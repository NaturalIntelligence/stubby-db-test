{
  "dbsets": "dbsets/",
  "mappings": {
    "default": {
      "request": {
        "method": "GET"
      },
      "response": {
        "strategy": "first-found",
        "latency": 0,
        "status": 200
      }
    },
    "requests": ["mappings/response.yaml"
                , "mappings/strategy.yaml"
                , "mappings/post.yaml"
                , "mappings/dbset.yaml"
                , "mappings/dbset2.yaml"
                , "mappings/attachment.yaml"]
  },
  "server": {
    "port": 9999
    ,"securePort" : 8000
    ,"mutualSSL" : true
    ,"ca" : ["truststore/ca/ca.crt"]
    ,"key" : "truststore/server.key"
    ,"cert" : "truststore/server.crt"
  },
  "stubs": "stubs/",
  "logs": {
    "path": "logs/"
  },
  "dumps": "dumps/"
}